{
  "version": 2,
  "builds": [
    {
      "src": "api/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/generate-test",
      "methods": ["POST"],
      "dest": "api/server.js"
    },
    {
      "src": "/store-test",
      "methods": ["POST"],
      "dest": "api/server.js"
    },
    {
      "src": "/fetch-test",
      "methods": ["GET"],
      "dest": "api/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
