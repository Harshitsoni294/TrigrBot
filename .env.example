# Example environment variables
# Copy this to .env and fill in your actual values

# Frontend Environment Variable (for Vite)
# This tells the frontend where your backend API is hosted
# Set this in Vercel dashboard: Settings > Environment Variables
VITE_API_BASE_URL=https://your-render-backend.onrender.com

# Backend Environment Variables (for server.cjs on Render)
# MongoDB Connection String
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/database

# Google Gemini API Key
GEMINI_API_KEY=your_gemini_api_key_here

# Server Port (for local development)
DEV_SERVER_PORT=4000
